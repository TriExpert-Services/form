{
  "name": "solicitudes-traduccion",
  "type": "application",
  "buildPath": "./",
  "dockerfile": "Dockerfile",
  "port": 80,
  "buildArgs": {
    "VITE_SUPABASE_URL": "${VITE_SUPABASE_URL}",
    "VITE_SUPABASE_ANON_KEY": "${VITE_SUPABASE_ANON_KEY}",
    "NODE_ENV": "production"
  },
  "healthCheck": {
    "enabled": true,
    "path": "/health",
    "interval": 30,
    "timeout": 10,
    "retries": 3
  },
  "env": {
    "NODE_ENV": "production"
  },
  "resources": {
    "memory": "512Mi",
    "cpu": "250m"
  }
}